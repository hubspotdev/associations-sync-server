{
  "name": "typescript-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "prod": "node dist/app.js",
    "start": "node dist/src/app.js",
    "build": "tsc",
    "seed": "tsx scripts/industry-prompt.ts && npx prisma db seed",
    "db-init": "npx prisma db push",
    "test": "jest --config jest.config.js --coverage",
    "test:routes": "jest --config jest.config.js \"src/__tests__/routes/.*\\.ts$\"",
    "test:specific": "jest --config jest.config.js src/__tests__/routes/definitionRoutes.test.ts --detectOpenHandles",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage-report": "jest --config jest.config.js --coverage && open coverage/lcov-report/index.html",
    "test:ci": "jest --config jest.config.js --ci --coverage --reporters='default' --reporters='jest-junit'"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hubspot/api-client": "12.0.1",
    "@prisma/client": "^3.15.2",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "inquirer": "^9.3.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui": "^5.18.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.11",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.0.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui": "^3.52.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^2.0.7",
    "prettier-plugin-prisma": "^3.14.0",
    "prisma": "^4.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.2",
    "typescript": "^4.1.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seeds/index.ts"
  }
}
